<form method="post" action="{{ path('community_delete', {'id': community.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ community.id) }}">
    <button class="btn btn-danger" href="#"> Delete <a class="fa fa-trash-o "> </a> </button>
</form>
